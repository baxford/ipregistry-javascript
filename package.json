{
  "name": "@ipregistry/client",
  "description": "Official Ipregistry Javascript Library (ES5, ES6+, TypeScript).",
  "version": "1.2.1",
  "main": "./dist/index.js",
  "browser": "./dist/browser/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "homepage": "https://ipregistry.co",
  "license": "Apache-2.0",
  "keywords": [
    "ipregistry",
    "ipdata",
    "ipgeo",
    "ip2location",
    "ipinfo",
    "ipinfodb",
    "ipthreat",
    "ipv4",
    "ipv6",
    "ip",
    "country",
    "currency",
    "ipaddress",
    "threat-data",
    "es5",
    "nodejs",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ipregistry/ipregistry-javascript.git"
  },
  "bugs": {
    "url": "https://github.com/ipregistry/ipregistry-javascript/issues"
  },
  "scripts": {
    "build:browser": "./node_modules/webpack-cli/bin/cli.js ",
    "build:cjs": "./node_modules/.bin/tsc -m commonjs",
    "build:mjs": "./node_modules/.bin/tsc -d --outDir dist/esm",
    "build": "yarn run lint && yarn run build:mjs && yarn run build:cjs && yarn run build:browser",
    "clean": "rm -rf dist/",
    "lint": "./node_modules/.bin/tslint -p tsconfig.json",
    "prepublish": "yarn run clean && yarn run build && yarn run test:integration",
    "pretest": "yarn run clean && yarn run build",
    "test:integration": "mocha -r ts-node/register integration_test/**/*.ts"
  },
  "author": "Ipregistry <support@ipregistry.co>",
  "devDependencies": {
    "@types/chai": "^4.2.4",
    "@types/lru-cache": "^5.1.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.7",
    "chai": "^4.2.0",
    "mocha": "^6.2.2",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "lru-cache": "^5.1.1"
  },
  "private": false
}
